% --------------- %
% ---  JOB    --- %
% --------------- %

Job: tHbb_v34_v1 % Rebinned config
  Label: "tH(bb) channel"
  CmeLabel: 13 TeV
  LumiLabel: 139 fb^{-1}
  POI: tH_NORM    # for fitting
  %POI: ttc_NORM  # for fitting
  POIPrecision: 3
  RankingPOIName: "#mu_{tH}"
  ReadFrom: NTUP
  MCstatThreshold: 0.01
  DebugLevel: 1
  PlotOptions: CHI2,NOXERR%,NORMSIG  %,YIELDS
  GetChi2: STAT+SYST
  ImageFormat: pdf  %,root
  Lumi: 1.
  MCweight: (36207.66*(runNumber<290000)+44307.4*(runNumber>=290000 && runNumber<310000)+58450.1*(runNumber>=310000))*weight_mc*xsec_weight*weight_pileup*weight_bTagSF_DL1r_Continuous*weight_jvt*weight_forwardjvt/totalEventsWeighted*weight_leptonSF
  NtupleName: nominal_Loose  # for histogramming
  CorrelationThreshold: 0.25
  CleanTables: TRUE
  UseATLASRounding: TRUE
  UseATLASRoundingTxt: TRUE
  UseATLASRoundingTex: TRUE
  SystCategoryTables: TRUE
  SystControlPlots: TRUE
  SystErrorBars: TRUE
  SystPruningShape: 0.01   # thresholds
  SystPruningNorm: 0.005   # thresholds
  SystLarge: 1.01   # thresholds
  RankingPlot: Merge
  RankingMaxNP: 20
%  StatOnly: TRUE
  BlindingThreshold: 0.00333  # what signal to bkr ratio needs to be
%  CustomAsimov: SM
  AllowWrongRegionSample: TRUE
  HistoChecks: NOCRASH
  ReplacementFile: config/Common_XS_unc_v34.txt
  LegendNColumns: 2
  UseGammaPulls: TRUE
%  GuessMCStatEmptyBins: TRUE
%  SmoothingOption: TTBARRESONANCE
  SplitHistoFiles: TRUE
  RatioYmax: 1.5
  RatioYmin: 0.5
  SeparationPlot: tH,ttH,ttb,ttc,ttlight
  Selection: @tau_pt.size()==0

% --------------- %
% ---  FIT  --- %
% --------------- %

Fit: myFit
  FitType: SPLUSB %BONLY
  FitRegion: CRSR %CRONLY 
%  doLHscan: R_tHttH,tH_NORM
%  LHscanSteps: 20
%  LHscanMin: 0
%  LHscanMax: 20
  BinnedLikelihoodOptimization: TRUE
  UseMinos: R_tHttH,tH_NORM,ttH_NORM,ttb_NORM,ttc_NORM
  %SetRandomInitialNPval: 0.1
  %SetRandomInitialNPvalSeed: 1
  SaturatedModel: FALSE
  GetGoodnessOfFit: TRUE
  FitBlind: TRUE % FALSE
  % BlindedParameters: tH_NORM

Limit: limit
  LimitBlind: TRUE
  SignalInjection:FALSE

Significance: significance
  SignificanceBlind: TRUE
  POIAsimov: 1.0


% --------------- %
% --- REGIONS --- %
% --------------- %
Region: SR
  Type: SIGNAL
  Variable: BDT[0],100,0.,1.
  VariableTitle: "BDT(tH)"
  Binning:0.,0.2732,0.3841,0.46,0.5409,0.6265,0.6962,0.7565,0.8176,0.8784,1.
  Selection: !(njets>=5 && nbjets>=4) && foam[1]==0.5
  Label: "SR"
  ShortLabel: "SR"
  LogScale: FALSE
  %LogScale: TRUE
  %Ymin: 1.
  %Ymax: 1.e8
  %Ymax: 16000
  DataType:DATA

Region: CR_ttb
  Type: CONTROL
  Variable: BDT[1],20,0.,1.
  Rebinning: 0,0.2,0.3,0.4,0.5,1.0
  VariableTitle: "BDT(t#bar{t} + #geq1b)"
  Selection: !(njets>=5 && nbjets>=4) && foam[1]==1.5
  Label: "CR t#bar{t} + #geq1b"
  ShortLabel: "CR-ttb"
  LogScale: FALSE
  %LogScale: TRUE
  %Ymin: 1.
  %Ymax: 1.e8
  %Ymax: 11000
  DataType:DATA

Region: CR_ttc
  Type: CONTROL
  Variable: BDT[2],20,0.,1.
  Rebinning: 0,0.25,0.3,0.4,1.0
  VariableTitle: "BDT(t#bar{t} + #geq1c)"
  Selection:!(njets>=5 && nbjets>=4) && foam[1]==2.5
  Label: "CR t#bar{t} + #geq1c"
  ShortLabel: "CR-ttc"
  LogScale: FALSE
  %LogScale: TRUE
  %Ymin: 1.
  %Ymax: 1.e8
  %Ymax: 12000
  DataType:DATA

Region: CR_ttlight
  Type: CONTROL
  Variable: BDT[3],20,0.,1.
  Rebinning: 0,0.3,0.4,0.5,0.6,1.0
  VariableTitle: "BDT(t#bar{t} + light)"
  Selection: !(njets>=5 && nbjets>=4) && foam[1]==3.5
  Label: "CR t#bar{t} + light"
  ShortLabel: "CR-ttlight"
  LogScale: FALSE
  %LogScale: TRUE
  %Ymin: 1.
  %Ymax: 1.e8
  %Ymax: 13000
  DataType:DATA

Region: CR_others
  Type: CONTROL
  Variable: BDT[4],20,0.,1.
  Rebinning: 0,0.2,0.3,0.4,0.5,1.0
  Selection: !(njets>=5 && nbjets>=4) && foam[1]>=4.5
  VariableTitle: "BDT(others)"
  Label: "CR others"
  ShortLabel: "CR-others"
  LogScale: FALSE
  %LogScale: TRUE
  %Ymin: 1.
  %Ymax: 1.e8
  %Ymax: 10000
  DataType:DATA

